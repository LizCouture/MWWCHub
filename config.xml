<?xml version='1.0' encoding='utf-8'?>
<widget id="software.geisel.mwwchub" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MWWCHub</name>
    <description>
        A schedule and feedback app for the Metrowest Conference for Women.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Geisel Software
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/img/applogo.png" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="cordova-plugin-console" spec="~1.0.8-dev" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
